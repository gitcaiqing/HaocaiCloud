    <!-- 统计所有数据 -->
    <select id="listData" resultMap="BaseResultMap">
        select <include refid="Base_Column_List"/>
        from tb_om_config
        where 1 = 1
        <!--<if test="basicUser.username != null and basicUser.username != ''">
