    <!--根据id查询-->
    <select id="selectById" resultType="com.haocai.base.cloudbase.entity.TbOmConfig" parameterType="java.lang.Long" >
        select *
        from tb_om_config
        where id = #{id,jdbcType=INTEGER}
    </select>

    <!-- 统计数据总数 -->
    <select id="countData" resultType="java.lang.Integer">
        select count(*)
        from tb_om_config
        where 1 = 1
