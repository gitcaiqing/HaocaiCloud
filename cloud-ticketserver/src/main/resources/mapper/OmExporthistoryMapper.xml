<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.haocai.ticketserver.cloudticketserver.mapper.OmExportHistoryMapper" >
    <resultMap id="BaseResultMap" type="com.haocai.base.cloudbase.entity.TbOmExportHistory" >
        <result column="id" property="id" jdbcType="INTEGER" />
        <result column="export_uuid" property="exportUuid" jdbcType="VARCHAR" />
        <result column="dops_version" property="exportUuid" jdbcType="VARCHAR" />
        <result column="pt_type" property="ptType" jdbcType="VARCHAR" />
